package ch21;

import java.awt.Graphics;

abstract public class Shape {
	 protected int x, y; 
	 
	 public Shape(int x, int y) {
		 this.x = x; 
		 this.y = y; 
	 }
	 
	 abstract public void draw(Graphics g);

	@Override
	public String toString() {
		return String.format("%d %d", x, y); 
	} 
	 
	 
}
